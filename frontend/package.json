{
  "name": "evaka-frontend",
  "version": "1.0.0",
  "description": "Frontend for eVaka",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "clean": "rm -rf dist screenshots videos node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "webpack --mode production",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve",
    "test": "jest src/*-frontend src/lib-*",
    "test:watch": "jest --watchAll src/*-frontend src/lib-*",
    "dev-maintenance-page": "cd src/maintenance-page-frontend && npx http-server",
    "e2e-test": "LANG=fi-FI LANGUAGE=fi_FI jest --runInBand src/e2e-test/specs",
    "e2e-ci": "LANG=fi-FI LANGUAGE=fi_FI BASE_URL=${BASE_URL:-http://localhost:9099} jest --runInBand --bail 1 --retries=2"
  },
  "dependencies": {
    "@evaka/eslint-plugin": "link:eslint-plugin",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@react-spring/rafz": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@sentry/browser": "^7.74.0",
    "@sentry/react": "^7.74.0",
    "@tanstack/react-query": "^5.0.5",
    "axios": "^1.5.0",
    "babel-plugin-styled-components": "^2.1.4",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "classnames": "^2.3.2",
    "compute-scroll-into-view": "^3.1.0",
    "core-js": "^3.33.0",
    "csstype": "^3.1.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "downshift": "^8.2.1",
    "history": "^5.3.0",
    "intersection-observer": "^0.12.2",
    "leaflet": "^1.9.4",
    "lib-common": "link:src/lib-common",
    "lib-components": "link:src/lib-components",
    "lib-customizations": "link:src/lib-customizations",
    "lib-icons": "link:src/lib-icons",
    "linkify-it": "^4.0.1",
    "lodash": "^4.17.21",
    "polished": "^4.2.2",
    "punycode": "^2.3.0",
    "react": "^18.2.0",
    "react-autosize-textarea": "^7.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-csv": "^2.2.2",
    "react-datepicker": "^4.20.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-focus-lock": "2.9.4",
    "react-image-crop": "^10.1.5",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.17.0",
    "react-select": "^5.7.3",
    "seamless-scroll-polyfill": "2.3.4",
    "styled-components": "6.1.0",
    "tslib": "^2.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.2",
    "@playwright/test": "^1.39.0",
    "@sentry/webpack-plugin": "^2.8.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.2",
    "@types/leaflet": "^1.9.3",
    "@types/linkify-it": "^3.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.8.0",
    "@types/punycode": "^2.1.0",
    "@types/react": "^18.2.14",
    "@types/react-csv": "^1.1.3",
    "@types/react-datepicker": "4.19.0",
    "@types/react-dom": "^18.2.6",
    "@types/react-linkify": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "csstype": "^3.1.2",
    "esbuild": "^0.19.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-expressions": "^1.3.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.5.0",
    "jest-circus": "^29.6.1",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-node": "^29.6.1",
    "jest-junit": "^16.0.0",
    "make-error-cause": "^2.3.0",
    "playwright": "^1.39.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "~5.2.2",
    "webpack": "5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-pwa-manifest": "^4.3.0",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0"
  },
  "peerDependenciesMeta": {
    "@fortawesome/pro-light-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-regular-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-solid-svg-icons": {
      "optional": true
    }
  },
  "resolutions": {
    "@types/react": "^18.0.23",
    "webpack@npm:5.37.1/terser-webpack-plugin": "5.1.1"
  },
  "engines": {
    "node": ">= 18.17.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Android versions",
    "last 2 FirefoxAndroid versions",
    "last 2 ChromeAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "> 5% in FI",
    "last 3 years"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "projects": [
      "./src/citizen-frontend",
      "./src/e2e-test",
      "./src/employee-frontend",
      "./src/lib-common",
      "./src/lib-components"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "jest-junit": {
    "outputDirectory": "test-results/",
    "addFileAttribute": "true"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/dist"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@evaka/recommended"
    ],
    "plugins": [
      "import",
      "react-hooks",
      "jsx-expressions",
      "lodash"
    ],
    "settings": {
      "import/internal-regex": "^(citizen-frontend|e2e-test|employee-frontend|employee-mobile-frontend|lib-common|lib-components|lib-customizations|lib-icons|maintenance-page-frontend)(/|$)",
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "import/order": [
        "warn",
        {
          "alphabetize": {
            "order": "asc"
          },
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always"
        }
      ],
      "react/jsx-curly-brace-presence": [
        "error",
        "never"
      ],
      "react/prop-types": "off",
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": true
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "(useApiState)"
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "prefer-arrow-callback": [
        "error",
        {
          "allowNamedFunctions": true
        }
      ],
      "arrow-body-style": [
        "error",
        "as-needed"
      ]
    },
    "overrides": [
      {
        "files": "**/*.{ts,tsx}",
        "extends": [
          "plugin:@typescript-eslint/recommended-type-checked",
          "plugin:@typescript-eslint/stylistic-type-checked"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
          "project": "./tsconfig.eslint.json"
        },
        "plugins": [
          "@typescript-eslint",
          "react-hooks"
        ],
        "rules": {
          "jsx-expressions/strict-logical-expressions": "error",
          "@typescript-eslint/no-misused-promises": [
            "error",
            {
              "checksVoidReturn": false
            }
          ],
          "@typescript-eslint/no-unused-vars": [
            "warn",
            {
              "argsIgnorePattern": "^_",
              "varsIgnorePattern": "^_"
            }
          ],
          "@typescript-eslint/consistent-type-definitions": "off",
          "@typescript-eslint/prefer-nullish-coalescing": "off",
          "@typescript-eslint/prefer-optional-chain": "off"
        }
      },
      {
        "files": "src/**/*.{js,jsx,ts,tsx}",
        "rules": {
          "lodash/import-scope": [
            "error",
            "method"
          ]
        }
      }
    ]
  },
  "packageManager": "yarn@3.6.1"
}
