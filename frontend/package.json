{
  "name": "evaka-frontend",
  "version": "1.0.0",
  "description": "Frontend for eVaka",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "clean": "rm -rf dist screenshots videos node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "webpack --mode production",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve",
    "test": "jest src/*-frontend src/lib-* eslint-plugin",
    "test:watch": "yarn test --watchAll",
    "dev-maintenance-page": "cd src/maintenance-page-frontend && npx http-server",
    "e2e-test": "LANG=fi-FI LANGUAGE=fi_FI jest --runInBand src/e2e-test/specs",
    "e2e-ci": "LANG=fi-FI LANGUAGE=fi_FI BASE_URL=${BASE_URL:-http://localhost:9099} jest --runInBand --retries=2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@react-spring/rafz": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@sentry/browser": "^9.12.0",
    "@sentry/react": "^9.12.0",
    "@tanstack/react-query": "^5.72.1",
    "autosize": "^6.0.1",
    "axios": "^1.8.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "classnames": "^2.5.1",
    "compute-scroll-into-view": "^3.1.0",
    "core-js": "^3.38.0",
    "csstype": "^3.1.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "downshift": "^9.0.4",
    "history": "^5.3.0",
    "ical-generator": "^8.1.1",
    "intersection-observer": "^0.12.2",
    "leaflet": "^1.9.4",
    "lib-common": "link:src/lib-common",
    "lib-components": "link:src/lib-components",
    "lib-customizations": "link:src/lib-customizations",
    "lib-icons": "link:src/lib-icons",
    "linkify-it": "^5.0.0",
    "lodash": "^4.17.21",
    "polished": "^4.3.1",
    "punycode": "^2.3.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^7.6.0",
    "react-day-picker": "^9.6.2",
    "react-dom": "^18.3.1",
    "react-focus-lock": "2.13.6",
    "react-focus-on": "^3.9.4",
    "react-image-crop": "^11.0.6",
    "react-leaflet": "^4.2.1",
    "react-router": "^7.3.0",
    "react-select": "^5.9.0",
    "seamless-scroll-polyfill": "2.3.4",
    "styled-components": "6.1.17",
    "tslib": "^2.8.0",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.24.0",
    "@evaka/eslint-plugin": "link:eslint-plugin",
    "@playwright/test": "^1.51.0",
    "@sentry/webpack-plugin": "^3.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/autosize": "^4.0.3",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.3",
    "@types/linkify-it": "^5.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.10.2",
    "@types/punycode": "^2.1.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.1",
    "csstype": "^3.1.2",
    "esbuild": "^0.25.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-expressions": "^1.3.2",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "express": "^4.21.2",
    "express-http-proxy": "^2.1.1",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "make-error-cause": "^2.3.0",
    "playwright": "^1.51.0",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.5.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack": "5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-pwa-manifest": "^4.3.0",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0"
  },
  "peerDependenciesMeta": {
    "@fortawesome/pro-light-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-regular-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-solid-svg-icons": {
      "optional": true
    }
  },
  "resolutions": {
    "@react-leaflet/core@npm:^2.1.0": "patch:@react-leaflet/core@npm%3A2.1.0#~/.yarn/patches/@react-leaflet-core-npm-2.1.0-de483ca53f.patch"
  },
  "engines": {
    "node": ">= 22.12.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Android versions",
    "last 2 FirefoxAndroid versions",
    "last 2 ChromeAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "> 5% in FI",
    "last 3 years"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "projects": [
      "./src/citizen-frontend",
      "./src/e2e-test",
      "./src/employee-frontend",
      "./src/lib-common",
      "./src/lib-components",
      "./eslint-plugin"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "jest-junit": {
    "outputDirectory": "test-results/",
    "addFileAttribute": "true"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.5.3"
}
