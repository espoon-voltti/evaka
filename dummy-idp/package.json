{
  "name": "dummy-idp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "type-check": "tsc --noEmit",
    "build": "npm run clean && npm install && tsc --build src",
    "dev": "tsc --build src && concurrently --prefix '[{name}]' --names 'tsc,nodemon' 'tsc --build --preserveWatchOutput -w src' 'NODE_ENV=local nodemon --enable-source-maps dist/index.js'"
  },
  "dependencies": {
    "express": "5.1.0",
    "express-session": "1.18.1",
    "samlp": "7.1.1",
    "tslib": "2.8.1",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "@types/express-session": "1.18.1",
    "@types/samlp": "1.0.11",
    "concurrently": "9.1.2",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "engines": {
    "node": ">= 22.12.0"
  }
}
